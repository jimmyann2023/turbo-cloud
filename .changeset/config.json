{
  "$schema": "https://unpkg.com/@changesets/config@3.0.0/schema.json",
  "changelog": [
    "@changesets/changelog-github",
    {
      "repo": "jimmyann2023/turbo-cloud"
    }
  ],
  "commit": false,
  "fixed": [["@repo-core/*", "@repo/*"]],
  "snapshot": {
    "prereleaseTemplate": "{tag}-{datetime}"
  },
  "privatePackages": {
    "version": true,
    "tag": true
  },
  "linked": [],
  "access": "public",
  "baseBranch": "main",
  "updateInternalDependencies": "patch",
  "ignore": []
}
